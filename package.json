{
  "name": "code-mood",
  "displayName": "Code Mood",
  "description": "A VS Code extension to detect code smells in Python files.",
  "version": "0.0.1",
  "publisher": "UG3Team8",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:python"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-mood.runSmellDetector",
        "title": "Run Code Smell Detector"
      },
      {
        "command": "code-mood.analyzeMood",
        "title": "Code Mood: Analyze Mood"
      },
      {
        "command": "code-mood.showTimeChart",
        "title": "Code Mood: Show Time Chart"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "dependencies": {
    "@huggingface/inference": "^3.6.1",
    "child_process": "^1.0.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0"
  }
}
